{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Ronald Macmaster <ronnymacmaster@gmail.com>",
  "scripts": {
    "start": "gatsby develop --host 0.0.0.0",
